<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<GridPane alignment="center" hgap="10" prefHeight="351.0" prefWidth="484.0" vgap="10" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controller">
    <columnConstraints>
        <ColumnConstraints />
    </columnConstraints>
    <rowConstraints>
        <RowConstraints />
    </rowConstraints>
    <children>
        <Pane prefHeight="351.0" prefWidth="409.0">
            <children>
                <ComboBox fx:id="boxCountry" layoutX="14.0" layoutY="93.0" onShowing="#chooseCountry" prefWidth="150.0" />
                <ComboBox fx:id="boxCity" layoutX="168.0" layoutY="93.0" onShowing="#chooseCity" prefWidth="150.0" />
                <Button fx:id="btnOk" layoutX="318.0" layoutY="254.0" mnemonicParsing="false" onAction="#getCityInfo" text="OK" />
                <Label layoutX="14.0" layoutY="68.0" text="Choose country" />
                <Label layoutX="168.0" layoutY="68.0" text="Choose city" />
                <Label layoutX="14.0" layoutY="151.0" text="Population:" />
                <Label layoutX="14.0" layoutY="176.0" text="Temperature:" />
                <Label layoutX="14.0" layoutY="199.0" text="Humidity:" />
                <Label layoutX="14.0" layoutY="222.0" text="Distance:" />
                <Label layoutX="164.0" layoutY="14.0" text="CITY INFO" textAlignment="CENTER">
                    <font>
                        <Font size="18.0" />
                    </font>
                </Label>
                <Label fx:id="lblPopulation" layoutX="97.0" layoutY="151.0" text=". . ." />
                <Label fx:id="lblTemperature" layoutX="97.0" layoutY="176.0" text=". . ." />
                <Label fx:id="lblHumidity" layoutX="97.0" layoutY="199.0" text=". . ." />
                <Label fx:id="lblDistance" layoutX="97.0" layoutY="222.0" text=". . ." />
            </children></Pane>
    </children>
</GridPane>
